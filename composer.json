{
    "name": "sierratecnologia/locaravel",
    "description": "Recursos para GeoLocalização",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "locaravel"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/locaravel/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/locaravel/issues",
        "source": "https://github.com/sierratecnologia/locaravel",
        "docs": "https://github.com/sierratecnologia/locaravel/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.0",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/pedreiro": "dev-master",
        "ricardosierra/validate": "dev-master",
        "ricardosierra/translation": "dev-master",
        "geo-io/wkb-parser": "^1.0",
        "jmikola/geojson": "^1.0",
        "ricardosierra/database": "^0.21",
        "jordanbrauer/unit-converter": "^0.8.4"
    },
    "suggest": {
        "igaster/laravel_cities": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Locaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/BaseTestCase.php",
            "tests/Stubs/"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "check-style": "phpcs -p --standard=PSR12 src --extensions=php",
        "fix-style": "phpcbf -p --standard=PSR12 src --extensions=php"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Locaravel\\LocaravelProvider"
            ],
            "dont-discover": [
                "scaffold-interface"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
